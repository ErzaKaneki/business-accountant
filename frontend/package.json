{
    "name": "business-finance-manager",
    "version": "1.0.0",
    "description": "Schedule C Tax Tracking & Financial Management",
    "main": "dist/main.js",
    "scripts": {
        "build": "webpack --mode production",
        "dev": "webpack --mode development --watch",
        "serve": "webpack serve --mode development",
        "type-check": "tsc --noEmit",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write src/**/*.ts",
        "clean": "rimraf dist",
        "start": "npm run build && python ../backend/app.py"
    },
    "keywords": [
        "finance",
        "tax",
        "schedule-c",
        "business",
        "typescript"
    ],
    "author": "Nevin Kadlec",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "css-loader": "^6.8.1",
        "eslint": "^8.45.0",
        "html-webpack-plugin": "^5.5.3",
        "mini-css-extract-plugin": "^2.7.6",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.1",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.4.4",
        "typescript": "^5.1.6",
        "webpack": "^5.88.2",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "dependencies": {
        "date-fns": "^2.30.0"
    }
}